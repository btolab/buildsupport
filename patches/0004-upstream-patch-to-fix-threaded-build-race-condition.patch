index 795b74a..219f0ce 100644
--- a/3rdparty/genie/src/actions/make/make_cpp.lua
+++ b/3rdparty/genie/src/actions/make/make_cpp.lua
@@ -86,7 +86,7 @@
 		end
 
 		-- target build rule
